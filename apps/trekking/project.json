{
  "name": "trekking",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/trekking/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/trekking",
        "index": "apps/trekking/src/index.html",
        "baseHref": "/",
        "main": "apps/trekking/src/main.tsx",
        "tsConfig": "apps/trekking/tsconfig.app.json",
        "assets": ["apps/trekking/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/trekking/webpack.config.ts"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/trekking/src/environments/environment.ts",
              "with": "apps/trekking/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "trekking:build",
        "hmr": true,
        "port": 4198
      },
      "configurations": {
        "development": {
          "buildTarget": "trekking:build:development"
        },
        "production": {
          "buildTarget": "trekking:build:production",
          "hmr": false
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "trekking:build",
        "watch": false,
        "port": 4198
      },
      "configurations": {
        "development": {
          "buildTarget": "trekking:build:development"
        },
        "production": {
          "buildTarget": "trekking:build:production"
        }
      }
    }
  },
  "tags": []
}
